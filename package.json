{
    "name": "london-Palitra-prod",
    "description": "A minimum, ral-color-centric theme for Ghost.",
    "demo": "https://anylight.ru",
    "version": "1.2.1",
    "engines": {
        "ghost": ">=2.0.0",
        "ghost-api": "v2"
    },
    "license": "MIT",
    "screenshots": {
        "desktop": "assets/screenshot-desktop.jpg",
        "mobile": "assets/screenshot-mobile.jpg"
    },
    "scripts": {
        "dev": "gulp",
        "zip": "gulp zip",
        "test": "gscan .",
        "pretest": "gulp build",
        "preship": "yarn test",
        "ship": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then yarn version && git push --follow-tags; fi"
    },
    "author": {
        "name": "Ghost Foundation and Evill",
        "email": "cs@anylight.ri",
        "url": "https://anylight.ru"
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "Minimal",
            "Magazine"
        ]
    },
    "keywords": [
        "ghost",
        "theme",
        "ghost-theme"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/VadimEv/London.git"
    },
    "bugs": "https://github.com/VadimEv/London/issues",
    "contributors": "https://github.com/TryGhost/London/graphs/contributors",
    "devDependencies": {
        "autoprefixer": "9.4.10",
        "cssnano": "4.1.10",
        "gscan": "^2.0.0",
        "gulp-livereload": "4.0.1",
        "gulp-minify": "^3.1.0",
        "gulp-postcss": "8.0.0",
        "gulp-sourcemaps": "2.6.5",
        "gulp-util": "3.0.8",
        "gulp-watch": "5.0.1",
        "gulp-webp": "^4.0.1",
        "gulp-zip": "4.2.0",
        "postcss-color-function": "4.0.1",
        "postcss-custom-properties": "8.0.9",
        "postcss-easy-import": "3.0.0"
    },
    "config": {
        "posts_per_page": 255,
        "image_sizes": {
            "xs": {
                "width": 100
            },
            "s": {
                "width": 300
            },
            "m": {
                "width": 600
            },
            "l": {
                "width": 1200
            },
            "xl": {
                "width": 2000
            }
        }
    },
    "dependencies": {
        "gulp": "^4.0.2",
        "gulp-uglify-es": "^2.0.0"
    }
}
